function to_submit(F) {
F.change_action.value = "";
F.submit_type.value = "";
F.save_button.value = sbutton.saving;
if (F._openvpn_certtype) {
(F._openvpn_certtype.checked == true) ?	F.openvpn_certtype.value = 1 : F.openvpn_certtype.value = 0;
}
apply(F);
}
function to_apply(F) {
F.change_action.value = "";
F.submit_type.value = "";
F.save_button.value = sbutton.saving;
if (F._openvpn_certtype) {
(F._openvpn_certtype.checked == true) ?	F.openvpn_certtype.value = 1 : F.openvpn_certtype.value = 0;
}
applytake(F);
}
