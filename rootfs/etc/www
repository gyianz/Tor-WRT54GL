{i}"hidden" name="submit_button" value="WL_ActiveTable-%s" />
{i}"hidden" name="action" value="Apply" />
{i}"hidden" name="change_action" value="gozila_cgi" />
{i}"hidden" name="submit_type" />
{i}"hidden" name="ifname" value="%s" />
{i}"hidden" name="commit" value="1" />

